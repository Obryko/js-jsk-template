{
  "name": "template",
  "version": "1.0.0",
  "description": "\"# js-jsk-template\"",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "prebuild": "shx rm -rf build/*",
    "build": "parcel build ./src/index.html -d build/ --public-url ./",
    "prestart": "shx rm -rf dist/*",
    "start": "parcel ./src/index.html -d dist/ --open",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "server": "json-server --watch db.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Obryko/js-jsk-template.git"
  },
  "author": "Kacper Obrycki",
  "license": "ISC",
  "homepage": "https://github.com/Obryko/js-jsk-template#readme",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "json-server": "^0.16.3",
    "parcel-bundler": "^1.3.1",
    "shx": "^0.3.3"
  },
  "resolutions": {
    "node-forge": "^0.10.0"
  }
}
